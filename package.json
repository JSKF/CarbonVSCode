{
    "author": {
        "name": "Rishaan Desai",
        "email": "contact@rishaandesai.me",
        "url": "https://rishaandesai.me"
    },
    "bugs": {
        "url": "https://github.com/Blackhole1123/CarbonVSCode/issues"
    },
    "contributors": [
        {
            "name": "Nathaniel Mejia",
            "url": "github.com/JSKF"
        }
    ],
    "funding": {
        "url": "https://github.com/Blackhole1123/sponsors"
    },
    "preview": true,
    "name": "carbon",
    "displayName": "Carbon",
    "description": "This is the official Carbon extension for Visual Studio Code.",
    "version": "0.0.1",
    "icon": "carbon-logo.png",
    "engines": {
        "vscode": "^1.69.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Blackhole1123/Carbon-VSCode-Extension"
    },
    "galleryBanner": {
        "color": "#000000",
        "theme": "dark"
    },
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "readme": "https://raw.githubusercontent.com/Blackhole1123/Carbon-VSCode-Extension/master/extension.md",
    "contributes": {
        "capabilities": {
            "hoverProvider": "true"
        },
        "languages": [
            {
                "id": "carbon",
                "aliases": [
                    "Carbon",
                    "carbon"
                ],
                "extensions": [
                    ".carbon"
                ],
                "configuration": "./language-configuration.json",
                "icon": {
                    "light": "./carbon-logo.png",
                    "dark": "./carbon-logo.png"
                }
            }
        ],
        "grammars": [
            {
                "language": "carbon",
                "scopeName": "source.carbon",
                "path": "./syntaxes/carbon.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "carbon",
                "path": "./snippets.json"
            }
        ]
    },
    "dependencies": {
        "vscode-languageclient": "^8.0.2"
    },
    "files.associations": {
		"*.carbon": "carbon"
	}
}
